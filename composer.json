{
    "name": "j6s/flow-transliterated-routes",
    "description": "Small package that ties in behat/transliterator with flows IdentityRoutePart to create smart international slugs",
    "type": "neos-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Hertenstein",
            "email": "hello@j6s.dev"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",

        "behat/transliterator": "^1.3.0",
        "neos/flow": "^6.1|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12.9"
    },
    "autoload": {
        "psr-4": {
            "J6s\\TransliteratedRoutes\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "J6s\\TransliteratedRoutes\\Tests\\": "Tests",
            "Neos\\Flow\\Tests\\": "Packages/Framework/Neos.Flow/Tests"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "Packages/Libraries"
    }
}
